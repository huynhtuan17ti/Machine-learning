    "def to_degree(x):\n",
    "    return x\n",
    "\n",
    "def grad(x):\n",
    "    return 2*x + 5*np.cos(x)\n",
    "\n",
    "def cost(x):\n",
    "    return x**2 + 5*np.sin(x)\n",
    "\n",
    "def myGD(ln_rate, x0):\n",
    "    x = [x0]\n",
    "    for cnt in range(1000):\n",
    "        x_new = x[-1] - ln_rate*grad(x[-1])\n",
    "        if abs(grad(x_new)) < 1e-3:\n",
    "            break\n",
    "        x.append(x_new)\n",
    "    return (x, cnt)"
   ]
